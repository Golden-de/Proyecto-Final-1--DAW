<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.CombatePokemon.MenuController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <ImageView fx:id="fondo" fitHeight="440.0" fitWidth="640.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <viewport>
            <Rectangle2D />
         </viewport>
         <image>
            <Image url="@../../../../../../Imagenes/pokemon.png" />
         </image>
      </ImageView>
      <Button fx:id="atras" layoutX="21.0" layoutY="325.0" mnemonicParsing="false" onAction="#Atras" prefHeight="25.0" prefWidth="89.0" text="Atras" />
      <Button fx:id="pokedex" layoutX="244.0" layoutY="325.0" mnemonicParsing="false" onAction="#abrir1" prefHeight="25.0" prefWidth="101.0" text="Pokedex" />
      <Button fx:id="entrenadores" layoutX="458.0" layoutY="325.0" mnemonicParsing="false" onAction="#abrir2" text="Entrenadores" />
      <TextArea fx:id="logs" layoutX="8.0" layoutY="365.0" prefHeight="101.0" prefWidth="624.0" />
      <ScrollBar layoutX="612.0" layoutY="366.0" orientation="VERTICAL" />
      <Button fx:id="entrar" layoutX="175.0" layoutY="127.0" mnemonicParsing="false" onAction="#entrarJuego" prefHeight="120.0" prefWidth="267.0" text="Jugar">
         <font>
            <Font size="45.0" />
         </font>
      </Button>
     
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</Pane>
