<?import javafx.scene.layout.Pane?>

<Pane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.CombatePokemon.PokedexController">
