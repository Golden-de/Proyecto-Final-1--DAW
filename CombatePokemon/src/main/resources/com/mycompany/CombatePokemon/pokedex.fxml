            <Rectangle2D />
         </viewport>
         <image>
            <Image url="@../../../../../../Imagenes/pokemon.png" />
         </image>
      </ImageView>
      <Button fx:id="atras" layoutX="37.0" layoutY="434.0" mnemonicParsing="false" onAction="#Atras" prefHeight="25.0" prefWidth="89.0" text="Atras" />
      <ChoiceBox fx:id="c" layoutX="476.0" layoutY="434.0" onAction="#cargarPokemon" prefWidth="150.0" />
      <ImageView fx:id="imagenPokemon" fitHeight="150.0" fitWidth="200.0" layoutX="113.0" layoutY="185.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="nombre" layoutX="431.0" layoutY="89.0" />
      <Label fx:id="vida" layoutX="431.0" layoutY="121.0" />
      <Label fx:id="ataque" layoutX="431.0" layoutY="153.0" />
      <Label fx:id="defensa" layoutX="431.0" layoutY="178.0" />
      <Label fx:id="ataqueEsp" layoutX="431.0" layoutY="205.0" />
      <Label fx:id="defensaEsp" layoutX="431.0" layoutY="232.0" />
      <Label fx:id="velocidad" layoutX="431.0" layoutY="260.0" />
      <Label fx:id="tipo" layoutX="430.0" layoutY="293.0" />
   </children>
   
</Pane>
