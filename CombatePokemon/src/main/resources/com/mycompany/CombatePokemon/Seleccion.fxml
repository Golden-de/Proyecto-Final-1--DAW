<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.CombatePokemon.SeleccionController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <ImageView fx:id="fondo" fitHeight="440.0" fitWidth="640.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <viewport>
            <Rectangle2D />
         </viewport>
         <image>
            <Image url="@../../../../../../Imagenes/pokemon.png" />
         </image>
      </ImageView>
      <Button fx:id="atras" layoutX="14.0" layoutY="310.0" mnemonicParsing="false" onAction="#Atras" prefHeight="25.0" prefWidth="89.0" text="Atras" />
      <TextArea fx:id="logs" editable="false" layoutX="7.0" layoutY="363.0" prefHeight="110.0" prefWidth="629.0" />
      <ChoiceBox fx:id="Dificultad" layoutX="469.0" layoutY="28.0" prefWidth="150.0" />
      <Button fx:id="Normas" layoutX="33.0" layoutY="28.0" mnemonicParsing="false" onAction="#Entrar" prefHeight="25.0" prefWidth="89.0" text="Normas" />
      <ChoiceBox fx:id="entrenadores" layoutX="460.0" layoutY="310.0" prefWidth="150.0" />
      <Button fx:id="empezar" layoutX="220.0" layoutY="148.0" mnemonicParsing="false" onAction="#juego" prefHeight="92.0" prefWidth="175.0" text="Empezar Partida">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
   
</Pane>
